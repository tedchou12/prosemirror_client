  {({ state: { state: { schema } } }) => <SchemaTab schema={schema} />}\n    </Subscribe>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"8da2751f5a4793dda55edfc5237e732e","cacheData":{"env":{}}}